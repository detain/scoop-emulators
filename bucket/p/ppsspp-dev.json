        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.15.4-1282-g5b9bdc134&platform=windows-amd64#/dl.7z",
            "hash": "ccd857caa2c0a9ca9f49410605377552fc22c420374fe8000983dfd1a4158367",
