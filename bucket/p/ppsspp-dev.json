            "shortcuts": [
                [
                    "PPSSPPWindows.exe",
                    "PPSSPP (dev)"
                ]
            ]
        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.15.4-991-g5a9a2bf6f&platform=windows-amd64#/dl.7z",
            "hash": "995d026f6b02ac2ad8f2bcf022e71a52508ff9400e98547e8fd444ef79b6fd26",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
                ]
            ],
