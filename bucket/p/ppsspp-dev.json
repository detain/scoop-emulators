        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.15.4-1064-gb2fbe1d8e&platform=windows-amd64#/dl.7z",
            "hash": "1408505504283239cd9615e05d14f4fc551359d9192d3fcdfccf04518118db6a",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
