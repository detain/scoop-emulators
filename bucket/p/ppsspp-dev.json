        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.14.2-3-gba2fab5b2&platform=windows-amd64#/dl.7z",
            "hash": "fa508e96013a05a13b99302b2bcc8319b12914d8f318051c1e7c152540904e30",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
                ]
