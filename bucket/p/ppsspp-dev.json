        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.16.6-949-g9fdcef9a0&platform=windows-amd64#/dl.7z",
            "hash": "ae50638aa8244200ac27b6c6888a1bfb770913cbee1f12585b16ee9a7d162ce1",
            "bin": [
