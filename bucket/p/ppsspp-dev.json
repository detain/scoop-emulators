                [
                    "PPSSPPWindows.exe",
                    "PPSSPP (dev)"
                ]
            ]
        },
        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.16.1-18-g99b2d2b2e&platform=windows-amd64#/dl.7z",
            "hash": "18a5a043970be043280d01b6579fdb5b6224d4e457935d6c1376013a2240a317",
            "bin": [
                [
                    "PPSSPPWindows64.exe",
                    "ppsspp-dev"
