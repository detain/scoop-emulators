        "64bit": {
            "url": "https://buildbot.orphis.net/ppsspp/index.php?m=dl&rev=v1.15.4-921-g2342c4522&platform=windows-amd64#/dl.7z",
            "hash": "b24557141fd598a811c3f426674412246e84113fbc25233c1c7de901be562b1f",
